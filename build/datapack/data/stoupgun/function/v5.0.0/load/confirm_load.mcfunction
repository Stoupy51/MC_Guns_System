## Define objectives
# Detect right click with a gun #TODO: alternate with carrot_on_a_stick
scoreboard objectives add stoupgun.right_click minecraft.used:minecraft.warped_fungus_on_a_stick

# Tracks right clicks to enable continuous right-click detection
scoreboard objectives add stoupgun.pending_clicks dummy

# Cooldown in ticks before being able to shot
scoreboard objectives add stoupgun.cooldown dummy

# Indicates if the player was zooming (used to remove slowness)
scoreboard objectives add stoupgun.zoom dummy

# Tracks the most recently selected weapon ID for weapon switching mechanics
scoreboard objectives add stoupgun.last_selected dummy

# Tracks the current amount of bullets in the selected weapon
scoreboard objectives add stoupgun.remaining_bullets dummy


# Define some constants
scoreboard players set #2 stoupgun.data 2
scoreboard players set #10 stoupgun.data 10
scoreboard players set #1000 stoupgun.data 1000
scoreboard players set #1000000 stoupgun.data 1000000

# Confirm load
tellraw @a[tag=convention.debug] {"translate":"stoupgun.loaded_stoupgun_v5_0_0","color":"green"}
scoreboard players set #stoupgun.loaded load.status 1

